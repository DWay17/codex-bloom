<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://ch.qos.logback/xml/ns/logback" xsi:schemaLocation="
	http://ch.qos.logback/xml/ns/logback https://raw.githubusercontent.com/enricopulatzo/logback-XSD/master/src/main/xsd/logback.xsd ">
	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d %-5p %c{17}.%M\(\): %m%n</pattern>
		</encoder>
	</appender>
	<root level="TRACE">
		<appender-ref ref="console" />
	</root>
</configuration>
